    "eslint-plugin-typescript": "^0.1.0",
    "istanbul": "^0.4.5",
    "jasmine": "^2.6.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "node-polyglot": "^2.2.2",
    "node-sass": "^4.5.3",
    "node-watch": "^0.5.5",
    "react": "^15.6.1",
    "react-router": "^4.1.1",
    "redux": "^3.7.1",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.7.8",
    "standard": "^10.0.2",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.2.2",
    "tslint": "^5.5.0",
    "tslint-config-standard": "^6.0.1",
    "typescript": "^2.4.1",
    "typescript-eslint-parser": "^3.0.0",
    "watch": "^1.0.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  }
