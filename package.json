{
  "name": "butterstocks",
  "version": "0.1.0",
  "description": "A cross-platform stock market analysis application for sound investment decisions ",
  "main": "index.js",
  "scripts": {
    "build": "npm run tslint && node node_modules/webpack/bin/webpack",
    "watch": "node node_modules/webpack/bin/webpack --progress --watch",
    "test-watch": "node ./node_modules/typescript/bin/tsc -p tsconfig.test.json --watch | node scripts/watch.js",
    "start": "npm run watch | node node_modules/webpack-dev-server/bin/webpack-dev-server",
    "test": "node ./node_modules/jasmine/bin/jasmine spec/specs.js",
    "test-build": "node ./node_modules/typescript/bin/tsc -p tsconfig.test.json",
    "tsfix": "node node_modules/tslint/bin/tslint -p tsconfig.json --fix",
    "tslint": "node node_modules/tslint/bin/tslint -p tsconfig.json",
    "tsfix-test": "node node_modules/tslint/bin/tslint -p tsconfig.test.json --fix",
    "tslint-test": "node node_modules/tslint/bin/tslint -p tsconfig.test.json"
  },
  "author": "Michel Moreau",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
